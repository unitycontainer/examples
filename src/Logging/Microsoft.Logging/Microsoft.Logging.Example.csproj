<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <ApplicationIcon />
    <OutputType>Exe</OutputType>
    <StartupObject />
    <MicrosoftLogging>..\..\..\..\Microsoft.Logging\src\Unity.Microsoft.Logging.csproj</MicrosoftLogging>
  </PropertyGroup>


  <ItemGroup Condition="Exists('$(MicrosoftLogging)')">
    <ProjectReference Include="$(MicrosoftLogging)" />
  </ItemGroup>

  <ItemGroup Condition="!Exists('$(MicrosoftLogging)')">
    <PackageReference Include="Unity.Microsoft.Logging" Version="6.0.*" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="6.0.0" />
  </ItemGroup>

</Project>
